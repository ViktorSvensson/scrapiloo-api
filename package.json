{
  "name": "@scrapiloo/api",
  "version": "0.0.8",
  "main": "dist/index.js",
  "private": false,
  "prettier": {
    "singleQuote": false,
    "bracketSpacing": false,
    "semi": true,
    "proseWrap": "preserve",
    "arrowParens": "always"
  },
  "scripts": {
    "debug": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "test:setup": "yarn add --dev jest @types/jest ts-jest",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src*/**/*.ts --fix",
    "lint:setup": "yarn add --dev eslint && eslint --init",
    "publish:patch": "yarn build && git add -A && git commit -m \"patch\" && npm publish --access public"
  },
  "author": {
    "name": "Carl Viktor Svensson",
    "url": "https://github.com/viktorsvensson"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@types/node": "^20.3.3",
    "typescript": "^4.1.3"
  }
}